  justify-content: center;
  align-items: center;
  margin-bottom: 20rpx;
}

.input-field {
  width: 600rpx;
  border-radius: 15rpx;
  border: 2rpx solid #ccc;
  background-color: #fff;
  font-size: 32rpx;
  color: #333;
  cursor: pointer;
  text-align: left;
}

.input-suggestion-list {
  width: 600rpx;
  height: max-content;
  position: absolute;
  top: 100%;
  padding-left: 20rpx;
  line-height: 60rpx;
  border-radius: 6rpx;
  box-shadow: 0rpx 0rpx 20rpx 0rpx #d9d9d9;
  color: #000000;
  background-color: #ffffff;
  z-index: 1;
}

.text {
  display: flex;
  color: #ccc;
  text-align: justify;
  width: 73%;
  font-size: 10.2px;
  padding-left: 100rpx;
}

.continueBtn {
  padding-top: 60rpx;
  width: 73%;
  align-items: center;
  padding-left: 100rpx;
}

.continueBtn button {
  border-radius: 15rpx;
  align-items: center;
  border-color: white;
  background-color: var(--claro-color);
}

.button.active {
  background-color: red;
  cursor: pointer;
}

.button.inactive {
  background-color: #ccc;
  cursor: not-allowed;
}

.dropdown {
  position: absolute;
  top: 100%;
  left: 0;
  width: 100%;
  max-height: 200px;
  overflow-y: auto;
  border: 1px solid #ccc;
  border-top: none;
