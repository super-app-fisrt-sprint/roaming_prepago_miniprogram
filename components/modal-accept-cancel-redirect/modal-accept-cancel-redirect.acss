  flex-direction: row-reverse;
  justify-content: space-around;
}

.footer-buttons{
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 10px;
  margin-bottom: 10px;
}

.button{
  width: 60%;
  background-color: #F6392C;
  border-radius: 40rpx;
  color: #ffffff;

}