
.ant-modal-body-footer-horizontal-primary {
  background-color: #F6392C;
  border-radius: 5pX;
  color: #ffffff;
  margin-left: 6px;
}

.link-text {

  color: blue ;
  cursor: pointer ; 
  text-decoration: underline ;
}

.ant-modal-body-content {
  padding-top: 5px;
  font-weight: bold;
  color: #000000;
}


.ant-modal-body-footer-horizontal {
  margin-top: 20px;
  padding: 10px;
}

.ant-modal-body {
  width: 370px;
}

.ant-modal-body-title {
  font-weight: bold;
